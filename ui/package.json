{
	"name": "ui",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"test": "npm run test:integration && npm run test:unit",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check . && eslint .",
		"format": "prettier --write .",
		"test:integration": "playwright test",
		"test:unit": "vitest",
		"update-sprite": "spreet sprite --sdf static/sprite_sdf && spreet --sdf --retina sprite static/sprite_sdf@2x",
		"update-api": "pnpm --filter @motis-project/motis-client generate"
	},
	"devDependencies": {
		"@eslint/compat": "^1.4.1",
		"@eslint/eslintrc": "^3.3.1",
		"@eslint/js": "^9.38.0",
		"@hey-api/openapi-ts": "^0.53.12",
		"@playwright/test": "^1.56.1",
		"@sveltejs/adapter-static": "^3.0.10",
		"@sveltejs/kit": "^2.48.4",
		"@sveltejs/vite-plugin-svelte": "^4.0.4",
		"@types/earcut": "^3.0.0",
		"@types/eslint": "^9.6.1",
		"@types/geojson": "^7946.0.16",
		"@types/mapbox__polyline": "^1.0.5",
		"@types/rbush": "^4.0.0",
		"@typescript-eslint/eslint-plugin": "^8.46.2",
		"@typescript-eslint/parser": "^8.46.2",
		"autoprefixer": "^10.4.21",
		"bits-ui": "^1.8.0",
		"clsx": "^2.1.1",
		"eslint": "^9.38.0",
		"eslint-config-prettier": "^10.1.8",
		"eslint-plugin-svelte": "^3.13.0",
		"globals": "^16.4.0",
		"postcss": "^8.5.6",
		"prettier": "^3.6.2",
		"prettier-plugin-svelte": "^3.4.0",
		"svelte": "^5.43.2",
		"svelte-check": "^4.3.3",
		"tailwind-merge": "^2.6.0",
		"tailwind-variants": "^0.3.1",
		"tailwindcss": "^3.4.18",
		"tailwindcss-animate": "^1.0.7",
		"tslib": "^2.8.1",
		"typescript": "^5.9.3",
		"typescript-eslint": "^8.46.2",
		"vite": "^5.4.21",
		"vitest": "^1.6.1"
	},
	"type": "module",
	"dependencies": {
		"@deck.gl/core": "~9.2.2",
		"@deck.gl/layers": "~9.2.2",
		"@deck.gl/mapbox": "~9.2.2",
		"@hey-api/client-fetch": "^0.4.4",
		"@lucide/svelte": "^0.548.0",
		"@mapbox/polyline": "^1.2.1",
		"@motis-project/motis-client": "workspace:^",
		"@tanstack/svelte-query": "^6.0.3",
		"@turf/boolean-point-in-polygon": "^7.2.0",
		"@turf/circle": "^7.2.0",
		"@turf/destination": "^7.2.0",
		"@turf/difference": "^7.2.0",
		"@turf/helpers": "^7.2.0",
		"@turf/rhumb-bearing": "^7.2.0",
		"@turf/rhumb-distance": "^7.2.0",
		"@turf/union": "^7.2.0",
		"colord": "^2.9.3",
		"earcut": "^3.0.2",
		"geojson": "^0.5.0",
		"maplibre-gl": "^5.10.0",
		"rbush": "^4.0.1",
		"svelte-radix": "^1.1.1"
	},
	"devEngines": {
		"packageManager": {
			"name": "pnpm",
			"onFail": "error"
		}
	}
}
