{
  "name": "rsl",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "dev": "vite",
    "dev-host": "vite --host",
    "build": "tsc && vite build",
    "serve": "vite preview",
    "serve-host": "vite preview --host",
    "lint": "eslint src --ext .js,.jsx,.ts,.tsx --max-warnings 0",
    "fix-lint": "eslint src --ext .js,.jsx,.ts,.tsx --fix",
    "format": "prettier --write src",
    "format-check": "prettier --check src",
    "ts-check": "tsc",
    "update-protocol": "node --trace-uncaught tools/protocol.js && eslint src/api/protocol --ext .ts --fix && prettier --write src/api/protocol"
  },
  "dependencies": {
    "@headlessui/react": "^1.7.4",
    "@headlessui/tailwindcss": "^0.1.1",
    "@heroicons/react": "^2.0.13",
    "@radix-ui/react-tooltip": "^1.0.2",
    "@tanstack/react-query": "^4.16.1",
    "@tanstack/react-query-devtools": "^4.16.1",
    "@visx/axis": "^2.14.0",
    "@visx/grid": "^2.12.2",
    "@visx/group": "^2.10.0",
    "@visx/responsive": "^2.10.0",
    "@visx/scale": "^2.2.2",
    "@visx/shape": "^2.12.2",
    "@visx/stats": "^2.10.0",
    "d3-array": "^3.2.0",
    "date-fns": "^2.29.3",
    "downshift": "^7.0.1",
    "jotai": "^1.10.0",
    "jotai-optics": "^0.2.0",
    "lodash-es": "^4.17.21",
    "optics-ts": "^2.4.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.39.5",
    "react-router-dom": "^6.4.3",
    "react-virtuoso": "^3.1.4",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@limegrass/eslint-plugin-import-alias": "^1.0.6",
    "@tailwindcss/forms": "^0.5.3",
    "@trivago/prettier-plugin-sort-imports": "^3.4.0",
    "@types/d3-array": "^3.0.3",
    "@types/d3-force": "^3.0.3",
    "@types/lodash-es": "^4.17.6",
    "@types/react": "^18.0.25",
    "@types/react-dom": "^18.0.9",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.44.0",
    "@typescript-eslint/parser": "^5.44.0",
    "@vitejs/plugin-react": "^2.2.0",
    "autoprefixer": "^10.4.13",
    "eslint": "^8.28.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-react": "^7.31.11",
    "eslint-plugin-react-hooks": "^4.6.0",
    "postcss": "^8.4.19",
    "prettier": "^2.7.1",
    "tailwindcss": "^3.2.4",
    "typescript": "^4.9.3",
    "vite": "^3.2.4"
  }
}
