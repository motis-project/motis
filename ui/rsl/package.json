{
  "name": "rsl",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "dev": "vite",
    "dev-host": "vite --host",
    "build": "tsc && vite build",
    "serve": "vite preview",
    "serve-host": "vite preview --host",
    "lint": "eslint src --ext .js,.jsx,.ts,.tsx --max-warnings 0",
    "fix-lint": "eslint src --ext .js,.jsx,.ts,.tsx --fix",
    "format": "prettier --write src",
    "format-check": "prettier --check src",
    "ts-check": "tsc",
    "update-protocol": "node --trace-uncaught tools/protocol.js && eslint src/api/protocol --ext .ts --fix && prettier --write src/api/protocol"
  },
  "dependencies": {
    "@headlessui/react": "^1.6.6",
    "@heroicons/react": "^1.0.6",
    "@radix-ui/react-tooltip": "^1.0.0",
    "@tanstack/react-query": "^4.0.10",
    "@tanstack/react-query-devtools": "^4.0.10",
    "@visx/axis": "^2.11.1",
    "@visx/grid": "^2.11.1",
    "@visx/responsive": "^2.10.0",
    "@visx/scale": "^2.2.2",
    "@visx/shape": "^2.11.1",
    "@visx/stats": "^2.10.0",
    "date-fns": "^2.28.0",
    "downshift": "^6.1.7",
    "jotai": "^1.7.6",
    "lodash-es": "^4.17.21",
    "optics-ts": "^2.3.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.33.1",
    "react-virtuoso": "^2.16.5",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@limegrass/eslint-plugin-import-alias": "^1.0.5",
    "@tailwindcss/forms": "^0.5.2",
    "@trivago/prettier-plugin-sort-imports": "^3.3.0",
    "@types/lodash-es": "^4.17.6",
    "@types/react": "^18.0.15",
    "@types/react-dom": "^18.0.6",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.30.7",
    "@typescript-eslint/parser": "^5.30.7",
    "@vitejs/plugin-react": "^2.0.0",
    "autoprefixer": "^10.4.7",
    "eslint": "^8.20.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-react": "^7.30.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "postcss": "^8.4.14",
    "prettier": "^2.7.1",
    "tailwindcss": "^3.1.6",
    "typescript": "^4.7.4",
    "vite": "^3.0.2"
  }
}
